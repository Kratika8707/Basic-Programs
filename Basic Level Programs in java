/******************************************************************************

                          WHETHER A NUMBER IS A PRIME NUMBER OR NOT

*******************************************************************************/
/*
public class Main
{
	public static void main(String[] args)
	{
		int n =9;
		int m = n/2;
		int flag =0;
		if(n==0 || n==1)
		  System.out.println("Not an prime number");
		else
		{
		    for(int i = 2;i<=m;i++)
		    {
		        if(n%i==0)
		        {
		          System.out.println("Not a prime number");
		          flag = 1;
		          break;
		        } 
		    }
		    if(flag == 0)
		       System.out.println(n+" "   + "Number is a prime number");
		}
	}
}
*/

/******************************************************************************
                            PALINDROME NUMBER OR NOT
                         
*******************************************************************************/
/*
public class Main
{
	public static void main(String[] args)
	{
		int n = 100;
		int temp, a, rev =0;
		temp = n;
		while(n>0)
		{
		    a = n%10;
		    rev = (rev*10) + a;
		    n = n/10;
		}
		if(temp==rev)
		  System.out.print( "Palindrome number");
		else
		  System.out.print( "Not a palindrome number");
	}
}
*/

