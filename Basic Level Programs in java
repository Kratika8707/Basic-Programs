/******************************************************************************
                            Print ASCII VALUE
                         
*******************************************************************************/

import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	   char ch1 = 'a';
	   int ascii = ch1;
	   int ch2  = 'b';
	   int val = 65;
       System.out.println(ascii);
       System.out.println(ch2);
       for(int i = 65; i <= 90; i++)  
       {  
        System.out.println(" The ASCII value of " + (char)i + "  =  " + i);  
       }  
	   
	
	}
}
