/******************************************************************************
                            ARMSTRONG NUMBER OR NOT
                         
*******************************************************************************/
/*
public class Main
{
	public static void main(String[] args)
	{
		int n = 100;
		int temp, a, sum =0;
		temp = n;
		while(n>0)
		{
		    a = n%10;
		    sum = sum + a*a*a;
		    n = n/10;
		}
		if(temp==sum)
		  System.out.print( "Armstrong number");
		else
		  System.out.print( "Not a Armstrong number");
	}
}
*/
