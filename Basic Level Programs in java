public class Main
{
    static int n1=0, n2=1 , n3;
    public static void printfibbonacci(int n)
    {
       
        if(n>0)
        {
            n3 =n1+n2;
            n1 = n2;
            n2 = n3;
            System.out.print(" " + n3);
            printfibbonacci(n-1);
        }
    }
